# Database
DATABASE_URL="postgresql://username:password@host:5432/devsure?schema=public"

# JWT
JWT_SECRET="your-super-secret-jwt-key-change-in-production"
JWT_EXPIRES_IN="7d"

# Server
PORT=5000
NODE_ENV=development

# Frontend URL (for CORS)
FRONTEND_URL="http://localhost:3000"

# Analysis Settings
ANALYSIS_TIMEOUT_MS=30000
ANALYSIS_POLL_INTERVAL_MS=10000
MAX_CONCURRENT_ANALYSES=1
# Google PageSpeed Insights API Key (optional but recommended)
# Get free key at: https://console.cloud.google.com/apis/credentials
# Without key: ~60 requests/day per IP
# With key: 25,000 requests/day
PAGESPEED_API_KEY=""

# Lighthouse Analysis (set to 'false' to disable)
ENABLE_LIGHTHOUSE=true